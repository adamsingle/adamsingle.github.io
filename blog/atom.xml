---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmins="https://www.w3.org/2005/atom">

    <title>Adam Single's DevBlog</title>
    <link href="http://adamsingle.github.io/blog/atom.xml" rel="self">
    <updated>{{site.time | date_to_xmlschema }}</updated>
    <id>http://adamsingle.github.io/blog</id>
    <author>
        <name>Adam Single</name>
        <email>adam@adamsingle.com</email>
    </author>

    {% for post in site.posts %}
        <entry>
            <title>{{post.title}}</title>
            <link href="http:adamsingle.github.io{{post.url}}"/>
            <updated>{{post.date | date_to_xmlschema }}</updated>
            <id>http://adamsingle.github.io{{post.id}}</id>
            <content type="html"> {{post.content | xml_escape}}</content>
        </entry>
    {% endfor %}
</feed>